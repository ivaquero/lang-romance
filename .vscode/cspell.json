{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "language": "en,fr,es",
    "languageSettings": [
        {
            "languageId": "markdown,txt",
            "caseSensitive": true
        }
    ],
    "dictionaries": [
        "custom-words",
        "french",
        "spanish"
    ],
    "dictionaryDefinitions": [
        {
            "name": "custom-words",
            "path": "../../backup/cspell/custom-words.txt",
            "addWords": true
        },
        {
            "name": "french",
            "path": "../../backup/cspell/french-words.txt"
        },
        {
            "name": "spanish",
            "path": "../../backup/cspell/spanish-words.txt"
        },
    ]
}
